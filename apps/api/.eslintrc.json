{
  "extends": ["../../packages/config/eslint-preset.js"],
  "ignorePatterns": ["dist", "build", "node_modules"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
